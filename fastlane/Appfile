app_identifier ENV["FABRIC_BUNDLE_IDENTIFIER"] # The bundle identifier of your app
apple_id ENV["FABRIC_DEVELOPER_EMAIL"] # Your Apple email address

team_id ENV["FABRIC_DEVELOPER_TEAM_ID"]  # Developer Portal Team ID

# you can even provide different app identifiers, Apple IDs and team names per lane:
# More information: https://github.com/fastlane/fastlane/blob/master/fastlane/docs/Appfile.md
